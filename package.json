{
  "name": "chives-light-node",
  "version": "0.1.8",
  "description": "Chivesweave Light Node",
  "email": "reactchives@gmail.com",
  "url": "https://github.com/chives-network/ChivesLightNode",
  "main": "main.js",
  "type": "module",
  "scripts": {
    "start": "npm run electron",
    "electron": "electron --max-old-space-size=8096  main.js",
    "express": "nodemon --max-old-space-size=8096 --expose-gc --inspect expressApp.js",
    "build": "electron-builder"
  },
  "keywords": [],
  "author": "Chivesweave <reactchives@gmail.com> (https://github.com/chives-network/ChivesLightNode)",
  "license": "GPL",
  "dependencies": {
    "@adobe/pdfservices-node-sdk": "^3.4.2",
    "@emotion/react": "^11.11.1",
    "@emotion/styled": "^11.11.0",
    "@mui/material": "^5.15.3",
    "arbundles": "^0.10.1",
    "arweave": "^1.14.4",
    "axios": "^1.6.2",
    "base64url": "^3.0.1",
    "concurrently": "^8.2.2",
    "cors": "^2.8.5",
    "electron-is-dev": "^2.0.0",
    "electron-log": "^5.0.3",
    "electron-settings": "^4.0.2",
    "electron-store": "^8.1.0",
    "express": "^4.19.2",
    "file-type": "^18.7.0",
    "fs": "^0.0.1-security",
    "html-pdf": "^3.0.1",
    "http": "^0.0.1-security",
    "https": "^1.0.0",
    "mammoth": "^1.6.0",
    "node-cron": "^3.0.3",
    "nodemon": "^3.1.0",
    "os": "^0.1.2",
    "puppeteer": "^21.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sharp": "^0.33.3",
    "sqlite3": "^5.1.7",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "electron": "^28.1.0",
    "electron-builder": "^24.9.1",
    "imagemin-jpegtran": "^7.0.0"
  },
  "build": {
    "appId": "org.chivesweave.lightnode",
    "productName": "Chives Light Node",
    "copyright": "Chives Network",
    "directories": {
      "output": "dist"
    },
    "linux": {
      "target": [
        "deb",
        "rpm",
        "AppImage"
      ],
      "category": "Network",
      "icon": "public/chives.png"
    },
    "win": {
      "icon": "public/chives.png",
      "target": [
        {
          "target": "nsis",
          "arch": [
            "x64"
          ]
        }
      ]
    },
    "asarUnpack": [
      "lib"
    ]
  }
}
